<h2>In Theaters:</h2>
<table class="movies">
  <tr>
    {{#each inTheaters}}
    <td>
      <a href={{this.link}} class="title"><h3>{{this.title}}</h3>
      <img src={{this.poster}}></a>
      <p>{{this.releaseDate}}</p>
      <p>{{this.description}}</p>
    </td>
    {{else}}
      <td>There's nothing in theaters</td>
    {{/each}}
  </tr>
</table>